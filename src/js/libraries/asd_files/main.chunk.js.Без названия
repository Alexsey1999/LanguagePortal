    }()
  }, {
    key: "requestPromise",
    value: function requestPromise(method, url, body) {
      return new Promise(function (resolve, reject) {
        var xhr = new XMLHttpRequest();
        xhr.open(method, url, true);

        xhr.onload = function () {
          try {
            resolve(JSON.parse(xhr.response));
          } catch (e) {
            reject({
              message: "Bad json format - ".concat(xhr.response)
            });
          }
        };

        xhr.send(body);

        xhr.onerror = function () {
          reject({
            message: "XHR request failed with code - ".concat(xhr.status, " - ").concat(xhr.statusText)
          });
        };
      });
    }
  }]);

  return XhrRequestClient;
}();

