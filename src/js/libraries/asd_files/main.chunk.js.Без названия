        title: action.payload.title,
        title_url: action.payload.title_url,
        more: action.payload.more,
        more_url: action.payload.more_url,
        tiles: action.payload.tiles.map(function (item) {
          return _objectSpread({}, item);
        })
      });

    case 'sort':
