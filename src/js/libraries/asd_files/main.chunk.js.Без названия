var currentModal = function currentModal(App) {
  return {
    get: function get() {
      return App.state.currentModal;
    },
    close: function close() {
      App.setState({
        currentModal: _Constants__WEBPACK_IMPORTED_MODULE_11__["MODAL_NULL"]
      });
    },
    set: function set(modal) {
      App.setState({
        currentModal: modal
      });

      if (modal && modal.id) {
        window.history.pushState(null, document.title, window.location.href);
      }
    },
    isActive: function isActive() {
      return App.state.currentModal && App.state.currentModal.id !== null;
    }
  };
};
