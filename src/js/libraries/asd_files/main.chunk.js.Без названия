  var pagesBlocksToCompare = page.blocks.map(function (b) {
    var block = _objectSpread({}, b);

    delete block.id;
    delete block.updated;
    return block;
  });
  var pagesEditBlocksToCompare = page.blocks_edit.map(function (b) {
    var block = _objectSpread({}, b);

    delete block.id;
    delete block.updated;
    return block;
  });
  return JSON.stringify(pagesBlocksToCompare) !== JSON.stringify(pagesEditBlocksToCompare);
};
