var GlobalTimer =
/*#__PURE__*/
function () {
  function GlobalTimer() {
    Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_classCallCheck__WEBPACK_IMPORTED_MODULE_0__["default"])(this, GlobalTimer);

    this.timer = void 0;
    this.timeout = void 0;
    this.timeRemaning = void 0;
    this.running = void 0;
    this.started = void 0;
    this.timer = null; // Current timer ID

    this.timeout = 1000 * 10; // Default delay time

    this.timeRemaning = 0; // How much time is left

    this.running = false; // Flag - is timer running or not

    this.started = 0; // Time when timer was started
  }

  Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_createClass__WEBPACK_IMPORTED_MODULE_1__["default"])(GlobalTimer, [{
    key: "set",
    value: function set() {
      var _this = this;

      this.running = true;
      this.started = new Date().getTime();

      if (this.timeRemaning === 0) {
        this.timeRemaning = this.timeout;
      }

      this.timer = setTimeout(function () {
        _this.unset();
      }, this.timeRemaning);
    }
  }, {
    key: "reset",
    value: function reset() {
      this.unset();
      this.set();
    }
    /**
     * 
     */

  }, {
    key: "pause",
    value: function pause() {
      this.running = false;
      clearTimeout(this.timer);
      this.timeRemaning -= new Date().getTime() - this.started;
    }
    /**
     * Full timer reset
     */

  }, {
    key: "unset",
    value: function unset() {
      if (this.timer) {
        clearTimeout(this.timer); // Clear current timer ID

        this.timer = null; // Delete the variable reference

        this.running = false;
        this.timeRemaning = 0;
        this.started = 0;
      }
    }
  }, {
    key: "check",
    value: function check() {
      return this.timer;
    }
  }, {
    key: "getTimeLeft",
    value: function getTimeLeft() {
      if (this.running) {
        this.pause();
        this.set();
      }

      return Math.ceil(this.timeRemaning / 1000);
    }
  }]);

  return GlobalTimer;
}();

