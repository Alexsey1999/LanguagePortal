function reducer(state, action) {
  var feedback = '';
  var newRows = state.rows.map(function (item) {
    return _objectSpread({}, item);
  });

  switch (action.type) {
    case 'title':
      return _objectSpread({}, state, {
        title: action.payload[0],
        title_url: action.payload[1]
      });

    case 'more':
      return _objectSpread({}, state, {
        more: action.payload[0],
        more_url: action.payload[1]
      });

    case 'newList':
      return _objectSpread({}, state, {
        rows: Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(action.payload)
      });

    case 'update':
      newRows[action.payload[0]].title = action.payload[1].title;
      newRows[action.payload[0]].descr = action.payload[1].feedback;
      newRows[action.payload[0]].img = action.payload[1].img;
      newRows[action.payload[0]].icon_id = action.payload[1].icon_id;
      return _objectSpread({}, state, {
        rows: newRows
      });

    case 'add':
      return _objectSpread({}, state, {
        rows: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.rows), [{
          img: action.payload[1].img,
          icon_id: action.payload[1].icon_id,
          title: action.payload[1].title,
          descr: action.payload[1].feedback,
          button: '',
          button_url: ''
        }])
      });

    case 'add_url':
      feedback = action.payload.feedback.replace(/\r?\n/g, " ");
      return _objectSpread({}, state, {
        rows: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.rows), [{
          img: action.payload.img,
          icon_id: "id".concat(action.payload.icon_id),
          title: action.payload.title,
          descr: feedback,
          button: ' ',
          button_url: action.payload.button_url
        }])
      });

    case 'delete':
      state.rows.splice(action.payload, 1);
      return _objectSpread({}, state);

    default:
      throw new Error();
  }
