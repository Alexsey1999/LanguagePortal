var messageErrors = {
  pointSale: function pointSale(type) {
    switch (type) {
      case 'title_error':
        return ' ';

      case 'title_url_error':
        return '  ';

      case 'descr_error':
        return ' ';

      case 'geo_error':
        return ' ';

      case 'time_error':
        return '  ';

      default:
        return '';
    }
  },
