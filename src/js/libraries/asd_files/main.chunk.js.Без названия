/***/ "./src/includes/Validators/WidgetTilesImgValidate.ts":
/*!***********************************************************!*\
  !*** ./src/includes/Validators/WidgetTilesImgValidate.ts ***!
  \***********************************************************/
/*! exports provided: default */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony default export */ __webpack_exports__["default"] = (function (items) {
  if (items.length > 0) {
    var hasIds = false;
    var hasImgs = false;

    if (items.filter(function (it) {
      return it.icon_id && it.icon_id.indexOf('id') === 0;
    }).length > 0 || items.filter(function (it) {
      return it.img === 'person';
    }).length > 0) {
      hasIds = true;
    }

    if (items.filter(function (it) {
      return it.icon_id && it.icon_id.indexOf('id') === -1 && it.img !== 'person';
    }).length > 0) {
      hasImgs = true;
    }

    if (hasImgs && hasIds) {
      return 'mixed';
    } else if (hasImgs && !hasIds) {
      return 'img';
    } else if (hasIds && !hasImgs) {
      return 'id';
    } else {
      return '';
    }
  }

  return '';
});

