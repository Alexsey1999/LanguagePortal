var getViewCountText = function getViewCountText(viewCount) {
  var text = '';
  var lastTwoDigits = parseInt(viewCount.toString().slice(-2), 10);

  if (lastTwoDigits === 11) {
    text = '';
  } else if (viewCount === 1 || viewCount % 10 === 1 && viewCount !== 11) {
    text = '';
  } else if (viewCount % 10 >= 2 && viewCount % 10 <= 4 && (viewCount < 12 || viewCount > 14)) {
    text = '';
  } else {
    text = '';
  }

  return "".concat(viewCount, " ").concat(text);
};
