/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isYouTubeUrl", function() { return isYouTubeUrl; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isYouTubeShortUrl", function() { return isYouTubeShortUrl; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isVimeoUrl", function() { return isVimeoUrl; });
