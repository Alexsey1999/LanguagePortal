  listCommon: function listCommon(widget) {
    var _this2 = this;

    var type = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'list';
    var status = true;
    var msg = messageErrors.default;
    var resp = {
      title: false,
      rows_count: false,
      rows: false,
      rows_imgs: true,
      rows_button: false,
      rows_item: []
    };
    var buttonsAreRequired = widget.has_buttons; //   

    var imagesAreRequired = widget.has_images; //   

    var textAreRequired = widget.has_text; //    

    var max_count_item = textAreRequired ? 3 : 6; //     

    if (textAreRequired) {
      if (widget.rows.length < 1) {
        resp.rows_count = false;
        status = false;
        msg = messageErrors.listCommon.countMin;
      }

      if (widget.rows.length > max_count_item) {
        resp.rows_count = false;
        status = false;
        msg = messageErrors.listCommon.countMax;
        return {
          status: status,
          resp: resp,
          msg: msg
        };
      }
    } else {
      if (widget.rows.length < 1 || widget.rows.length > max_count_item) {
        resp.rows_count = false;
        status = false;
        msg = messageErrors.listCommon.count;
      }
    } //   


    if (WidgetValidate.isTitle(widget.title)) {
      resp.title = true;
      status = false;
    } //    -       


    if (imagesAreRequired) {
      if (Object(_WidgetTilesImgValidate__WEBPACK_IMPORTED_MODULE_2__["default"])(widget.rows) === 'mixed') {
        status = false;
        resp.rows_imgs = false;
        msg = _ErrorMessages__WEBPACK_IMPORTED_MODULE_3__["TILES_IMAGES_ARE_NOT_THE_SAME_TYPE"];
      }
    }

    if (widget.rows.length > 0) {
      var bt = 0;
      widget.rows.forEach(function (item) {
        var card = {
          img: false,
          title: false,
          descr: false,
          button: false,
          geo: false,
          time: false,
          text: false
        };

        if (WidgetValidate.isTitle(item.title)) {
          card.title = true;
          status = false;
        }

        if (imagesAreRequired) {
          // ,      id 
          if (item.img === '' || item.icon_id === '') {
            status = false;
            card.img = true;
            resp.rows = true;
          }
        }

        if (textAreRequired) {
          if (item.text == '') {
            card.text = true;
            status = false;
          }
        } //   


        if (item.button.trim() == '' || item.button_url == '') {
          //     
          if (buttonsAreRequired) {
            // ,    
            if (_this2.checkRowsHasFullButtons(widget.rows)) {
              //    -   
              bt++;
              card.button = true;
            }
          }
        } //    


        if (item.url !== undefined && item.url.trim() === '') {
          //  -   
          if (_this2.checkRowsHasFilledTitleUrls(widget.rows)) {
            //    
            msg = messageErrors.coverList.url;
            card.title = true;
            status = false;
          }
        }

        if (buttonsAreRequired) {
          if (item.button.trim() == '') {
            card.button = true;
            status = false;
          }

          ;
        }

        resp.rows_item.push(card);
      });

      if (widget.rows.length != bt && bt != 0) {
        status = false;
        resp.rows = true;
        resp.rows_button = true;
        msg = messageErrors.list.button;
      }
    }

    return {
      status: status,
      resp: resp,
      msg: msg
    };
  },
  isTitle: function isTitle(title) {
    if (title.trim() === '' || title == null || title === undefined) return true;
    if (title.length > _Config__WEBPACK_IMPORTED_MODULE_1__["default"].widgetLength.title) return true;
    return false;
  },
  isText: function isText(title, length) {
    if (title === null || title === undefined || title.trim() == '') return true;
    if (title.length > length) return true;
    return false;
  },
  isErrors: function isErrors(errors, id, type) {
    if (errors.rows_item.length <= id) return false;

    switch (type) {
      case 'title':
        return errors.rows_item[id].title;

      case 'img':
        return errors.rows_item[id].img;

      case 'descr':
        return errors.rows_item[id].descr;

      case 'geo':
        return errors.rows_item[id].geo;

      case 'time':
        return errors.rows_item[id].time;

      case 'button':
        return errors.rows_item[id].button;

      case 'text':
        return errors.rows_item[id].text;
    }

    return false;
  },
  PointSale: function PointSale(widget) {
    if (widget.code.more != '') if (widget.code.more_url == '') return false;
    if (widget.code.title == '') return false;
    return true;
  },
  checkRowsHasFullButtons: function checkRowsHasFullButtons(rows) {
    var fullButtons = rows.filter(function (row) {
      if (typeof row.button === 'undefined' || typeof row.button_url === 'undefined') {
        return false;
      }

      var buttonText = row.button.trim();
      var buttonUrl = row.button_url.trim();
      return buttonText !== '' && buttonUrl !== '';
    });
    return fullButtons.length > 0;
  },
  checkRowsHasFilledTitleUrls: function checkRowsHasFilledTitleUrls(rows) {
    var filledTitleUrls = rows.filter(function (row) {
      if (typeof row.url === 'undefined') {
        return false;
      }

      var titleUrl = row.url.trim();
      return titleUrl !== '';
    });
    return filledTitleUrls.length > 0;
  }
};
