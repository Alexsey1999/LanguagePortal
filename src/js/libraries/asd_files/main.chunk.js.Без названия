/*! exports provided: isYouTubeUrl, isYouTubeShortUrl, isVimeoUrl, isUrl, isValidBannerUrl, isValidYouTubeVideoUrl, default */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
