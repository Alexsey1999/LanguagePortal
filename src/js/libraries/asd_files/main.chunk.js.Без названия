      var retries = 0;
      var self = this;
      return function makeRequest() {
        self.request('POST', url, data).then(function (result) {
          callback(result);
          retries = 0;
        }).catch(function (error) {
          if (retries < self.retries) {
            retries++;
            setTimeout(function () {
              makeRequest();
            }, self.retryTimeout);
            return;
          }

          callback({
            success: false,
            message: error.message
          });
          self.logger.error(error, 'runtime_error', 'GetSubscriptions.jsx');
