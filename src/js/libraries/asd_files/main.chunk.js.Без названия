  var onSwipeMove = function onSwipeMove(id, position, event) {
    var swipedX = Math.abs(position.x);
    var row = document.querySelector("[data-key=\"".concat(id, "\"]"));
    row.classList.remove('widget-list-wrap-transition');

    if (swipedX >= _includes_Constants__WEBPACK_IMPORTED_MODULE_21__["BUTTON_WIDTH"]) {
      swiped = _includes_Constants__WEBPACK_IMPORTED_MODULE_21__["BUTTON_WIDTH"];
      var value = position.x >= 0 ? _includes_Constants__WEBPACK_IMPORTED_MODULE_21__["BUTTON_WIDTH"] : -_includes_Constants__WEBPACK_IMPORTED_MODULE_21__["BUTTON_WIDTH"];
      row.style.transform = "translateX(".concat(value, "px)");
      row.classList.add('active');
      return false;
    } else {
      row.classList.remove('active');
    }

    row.style.transform = "translateX(".concat(position.x, "px)");
    swiped = swipedX;
  };

  var onSwipeEnd = function onSwipeEnd(event, widget) {
    var button = document.querySelector("[data-key=\"".concat(widget.id, "\"]"));
    button.classList.add('widget-list-wrap-transition');
    var isActive = button.classList.contains('active');

    if (swiped === 0 && !isActive) {
      changeWidgetEdit(widget);
    } else if (swiped < _includes_Constants__WEBPACK_IMPORTED_MODULE_21__["BUTTON_WIDTH"]) {
      button.style.transform = "translateX(0px)";
      button.classList.remove('active');
    }

    swiped = 0;
  };

