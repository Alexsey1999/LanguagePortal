function reducer(state, action) {
  var newRows = [];

  switch (action.type) {
    case 'title':
      return _objectSpread({}, state, {
        title: action.payload[0],
        title_url: action.payload[1]
      });

    case 'more':
      return _objectSpread({}, state, {
        more: action.payload[0],
        more_url: action.payload[1]
      });

    case 'img':
      state.rows[action.payload.cardID].img = action.payload.img;
      state.rows[action.payload.cardID].icon_id = action.payload.img_vk;
      return _objectSpread({}, state);

    case 'button':
      state.rows[action.payload[0]].button = action.payload[1][0];
      state.rows[action.payload[0]].button_url = action.payload[1][1];
      return _objectSpread({}, state);

    case 'item':
      state.rows[action.payload[0]].title = action.payload[1].title;
      state.rows[action.payload[0]].url = action.payload[1].url;
      state.rows[action.payload[0]].descr = action.payload[1].descr;
      state.rows[action.payload[0]].geo = action.payload[1].geo;
      state.rows[action.payload[0]].time = action.payload[1].time;
      state.rows[action.payload[0]].text = action.payload[1].text;
      return _objectSpread({}, state);

    case 'add':
      return _objectSpread({}, state, {
        rows: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.rows), [{
          button: "",
          button_url: "",
          descr: "",
          icon_id: "",
          img: "",
          title: "",
          url: "",
          text: "",
          geo: "",
          time: ""
        }])
      });

    case 'delete':
      state.rows.splice(action.payload, 1);
      return _objectSpread({}, state);

    case 'update':
      return _objectSpread({}, state, {
        rows: Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(action.payload)
      });

    case 'has_images':
    case 'has_buttons':
    case 'has_text':
      return _objectSpread({}, state, Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_defineProperty__WEBPACK_IMPORTED_MODULE_1__["default"])({}, action.type, action.payload));
    //     

    case 'sub':
