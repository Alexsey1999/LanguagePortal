var WithHoverTooltip = function WithHoverTooltip(props) {
  var className = props.className,
      tooltipContent = props.tooltipContent,
      children = props.children;
  var rootElem = Object(react__WEBPACK_IMPORTED_MODULE_0__["useRef"])(null);

  var renderTooltip = function renderTooltip() {
    if (rootElem.current) {
      /**
       *   
       */
      var contentElem = document.createElement('div');
      contentElem.classList.add('WithTooltip__content');
      contentElem.classList.add('WithTooltip__content_hover');
      var arrowElem = document.createElement('div');
      arrowElem.classList.add('WithTooltip__content_hover-arrow');
      /**
       *     
       */

      rootElem.current.appendChild(contentElem);
      rootElem.current.appendChild(arrowElem);
      /**
       *   ReactDom.render      ReactElement
       */

      react_dom__WEBPACK_IMPORTED_MODULE_1___default.a.render(tooltipContent, contentElem);
      /**
       * 
       */

      var coords = rootElem.current.getBoundingClientRect();
      var contentCoords = contentElem.getBoundingClientRect();
      var windowWidth = document.body.clientWidth;
      contentElem.style.bottom = "".concat(coords.height + 10, "px");
      arrowElem.style.left = "".concat(coords.width / 2 - 4, "px");
      arrowElem.style.bottom = "".concat(coords.height + 4, "px"); //          

      var tooltipEnds = contentCoords.left + contentCoords.width; //        ( ,        )

      var tooltipLeftPosInElement = contentCoords.width / 2 - coords.width / 2; //            

      var tooltipStartComputed = contentCoords.left - tooltipLeftPosInElement; //      

      var tooltipEndComputed = tooltipStartComputed + contentCoords.width;

      if (tooltipEndComputed > windowWidth) {
        //        
        //      + 10     
        contentElem.style.left = "-".concat(tooltipEnds - windowWidth + 5, "px");
      } else if (tooltipStartComputed <= 0) {
        //       
        //      + 10     
        contentElem.style.left = "-".concat(tooltipLeftPosInElement - Math.abs(tooltipStartComputed) - 5, "px");
      } else {
        //     -     
        contentElem.style.left = "-".concat(tooltipLeftPosInElement, "px");
      }

      contentElem.classList.add('opened');
      arrowElem.classList.add('opened');
    }
  };

  var removeTooltip = function removeTooltip() {
    var c = rootElem.current.querySelector('.WithTooltip__content');
    var a = rootElem.current.querySelector('.WithTooltip__content_hover-arrow');
    if (c) rootElem.current.removeChild(c);
    if (a) rootElem.current.removeChild(a);
  };

  var classNames = ['WithTooltip WithTooltip_hover'];

  if (className) {
    classNames.push(className);
  }

  return react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement("div", {
    onMouseEnter: renderTooltip,
    onMouseLeave: removeTooltip,
    ref: rootElem,
    className: classNames.join(' '),
    __source: {
      fileName: _jsxFileName,
      lineNumber: 112
    },
    __self: this
  }, children);
};

