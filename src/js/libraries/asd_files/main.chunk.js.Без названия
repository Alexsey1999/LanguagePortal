          if (this.gets.hasOwnProperty(name)) {
            this.params[name] = this.gets[name];
          }
        }

        this.hash = this.get_hash_params();

        for (var _name in this.hash) {
          if (this.hash.hasOwnProperty(_name)) {
            this.params[_name] = this.hash[_name];
          }
