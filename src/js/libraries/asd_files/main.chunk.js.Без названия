    key: "getBase64MimeType",
    value: function getBase64MimeType(encoded) {
      var mime = 'image/jpeg';
      var ext = 'jpg';

      if (typeof encoded !== 'string') {
        return {
          mime: mime,
          ext: ext
        };
      }

      var match = encoded.match(/data:([a-zA-Z0-9]+\/[a-zA-Z0-9-.+]+).*,.*/);

      if (match && match.length) {
        mime = match[1];
        ext = mime.split('/')[1];

        if (ext === 'jpeg') {
          ext = 'jpg';
        }
      }

      return {
        mime: mime,
        ext: ext
      };
    }
  }]);

