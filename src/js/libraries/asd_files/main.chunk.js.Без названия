var resizer = function resizer(App, UrlParams, Logger) {
  return {
    //   iframe       
    //          state
    // +    ,     -
    //     ,      
    setHeight: function setHeight() {
      var subscriptionPageAsDefault = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : false;

      try {
        var body = document.body; //    

        var contentInner = document.querySelector('.content--inner');

        if (_vkontakte_vk_bridge__WEBPACK_IMPORTED_MODULE_8___default.a.supports("VKWebAppResizeWindow") && UrlParams.params.vk_platform === 'desktop_web') {
          //  ,     ,    4050px
          var fitDisplayHeight = Math.min(_Config__WEBPACK_IMPORTED_MODULE_16__["default"].max_app_height, Math.min(App.appInitialHeight, window.outerHeight - 300)) + 55;
          var resultAppHeight = fitDisplayHeight;
          var list = App.location.getList(); //   

          var isSingle = !!App.location.getSid() || !!App.state.subscription_id;

          if (contentInner && list !== 'widgets' && isSingle || //    
          subscriptionPageAsDefault || //     -
          App.state.route === 'landing') {
            //  
            if (UrlParams.params.vk_platform === 'desktop_web') {
              body.style.overflowY = 'hidden';
            }

            if (contentInner) {
              var realContentHeight = contentInner.offsetHeight;
              var extraHeight = App.state.route === 'landing' ? 80 : 120;
              resultAppHeight = Math.min(realContentHeight + extraHeight, _Config__WEBPACK_IMPORTED_MODULE_16__["default"].max_app_height);
            }
          } else {
            body.style.overflowY = 'auto';
          }

          _vkontakte_vk_bridge__WEBPACK_IMPORTED_MODULE_8___default.a.send("VKWebAppResizeWindow", {
            "height": resultAppHeight,
            "width": _Config__WEBPACK_IMPORTED_MODULE_16__["default"].app_width
          });
        } else {
          console.log('Event VKWebAppResizeWindow not supported');
        }
      } catch (e) {
        Logger.error(e, 'set_height_error', 'App.js');
      }
    },
    setLandingHeight: function setLandingHeight() {
      try {
        var body = document.body; //    

        var contentInner = document.querySelector('.content--inner');

        if (_vkontakte_vk_bridge__WEBPACK_IMPORTED_MODULE_8___default.a.supports("VKWebAppResizeWindow") && UrlParams.params.vk_platform === 'desktop_web') {
          //  ,     ,      
          var fitDisplayHeight = Math.min(_Config__WEBPACK_IMPORTED_MODULE_16__["default"].max_landing_app_height, Math.min(App.appInitialHeight, window.outerHeight - 300)) + 55;
          var resultAppHeight = fitDisplayHeight;

          if (App.state.route === 'page') {
            //  
            body.style.overflowY = 'hidden';

            if (contentInner) {
              var realContentHeight = contentInner.offsetHeight;
              resultAppHeight = Math.min(realContentHeight, _Config__WEBPACK_IMPORTED_MODULE_16__["default"].max_landing_app_height);
            }
          } else {
            body.style.overflowY = 'auto';
          }

          _vkontakte_vk_bridge__WEBPACK_IMPORTED_MODULE_8___default.a.send("VKWebAppResizeWindow", {
            "height": resultAppHeight,
            "width": _Config__WEBPACK_IMPORTED_MODULE_16__["default"].app_width
          });
        } else {
          console.log('Event VKWebAppResizeWindow not supported');
        }
      } catch (e) {
        Logger.error(e, 'set_landing_height_error', 'App.js');
      }
    }
  };
};
