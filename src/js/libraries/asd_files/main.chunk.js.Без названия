        return _objectSpread({}, item);
      });
      newTiles[action.payload.cardID].img = action.payload.img;
      newTiles[action.payload.cardID].icon_id = action.payload.img_vk;
      return _objectSpread({}, state, {
        tiles: newTiles
      });

    case 'item':
