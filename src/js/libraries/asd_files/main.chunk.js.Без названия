      newRows = state.rows.map(function (row) {
        return _objectSpread({}, row);
      });
      newRows[item_id].button = action.payload.data.button;
      newRows[item_id].button_url = action.payload.data.button_url;
      return _objectSpread({}, state, {
        rows: newRows
      });

    default:
      throw new Error("Unsupported type ".concat(action.type));
  }
