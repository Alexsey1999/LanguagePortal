      if (this.inited) {
        return false;
      }

      this.id = config.id;
      var t = document.createElement('script');
      t.type = 'text/javascript';
      t.async = true;
      t.src = 'https://vk.com/js/api/openapi.js?162';

      t.onload = function () {
        if (window.VK) {
          _this.inited = true;
          console.log('Init vk_pixel', _this.id);
          window.VK.Retargeting.Init(_this.id);
          window.VK.Retargeting.Hit();
          config.onLoad && config.onLoad();
        }
      };

      document.head.appendChild(t);
    }
  }, {
    key: "hit",
    value: function hit() {
