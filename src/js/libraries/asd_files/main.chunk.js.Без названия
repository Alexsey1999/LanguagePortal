/***/ "./src/omponents/Pages/Blocks/Edit/ButtonCommon/validator.ts":
/*!********************************************************************!*\
  !*** ./src/omponents/Pages/Blocks/Edit/ButtonCommon/validator.ts ***!
  \********************************************************************/
/*! exports provided: default, validator */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "default", function() { return validator; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "validator", function() { return validator; });
/* harmony import */ var _includes_Helpers_Helpers__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! ../../../../../includes/Helpers/Helpers */ "./src/includes/Helpers/Helpers.ts");

function validator(state) {
  var result = {
    status: true,
    errors: {}
  };

  if (state.text.trim() === '') {
    result.status = false;
    result.errors.text = ' ';
  } else {
    delete result.errors.text;
  }

  if (state.action === 'url') {
    if (!state.url) {
      result.status = false;
      result.errors.url = ' ';
      return result;
    }

    if (state.url.trim() === '') {
      result.status = false;
      result.errors.url = ' ';
    } else {
      delete result.errors.url;
    }

    if (!Object(_includes_Helpers_Helpers__WEBPACK_IMPORTED_MODULE_0__["isValidLink"])(state.url)) {
      result.status = false;
      result.errors.url = '  ';
    } else {
      delete result.errors.url;
    }
  }

  if (state.action === 'lead') {
    if (!state.lead_admin) {
      result.status = false;
      result.errors.lead_admin = ' ';
    }
  }

  if (state.action === 'subscribe') {
    if (!state.subscription_id) {
      result.status = false;
      result.errors.subscription = '  ';
      return result;
    }
  }

  if (state.action === 'bot') {
    if (!state.bot_id) {
      result.status = false;
      result.errors.bot_id = ' ';
      return result;
    }
  }

  return result;
}
;


