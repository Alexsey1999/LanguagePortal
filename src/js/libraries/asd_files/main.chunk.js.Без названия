function reducer(state, action) {
  var newRows = state.rows.map(function (item) {
    return _objectSpread({}, item);
  });

  switch (action.type) {
    case 'title':
      return _objectSpread({}, state, {
        title: action.payload[0],
        title_url: action.payload[1]
      });

    case 'more':
      return _objectSpread({}, state, {
        more: action.payload[0],
        more_url: action.payload[1]
      });

    case 'button':
      state.rows[action.payload[0]].button = action.payload[1][0];
      state.rows[action.payload[0]].button_url = action.payload[1][1];
      return _objectSpread({}, state);

    case 'img':
      state.rows[action.payload.cardID].img = action.payload.img;
      state.rows[action.payload.cardID].img_vk = action.payload.img_vk;
      return _objectSpread({}, state);

    case 'item':
      state.rows[action.payload[0]].title = action.payload[1].title;
      state.rows[action.payload[0]].descr = action.payload[1].descr;
      state.rows[action.payload[0]].url = action.payload[1].url;
      return _objectSpread({}, state);

    case 'createCard':
      return _objectSpread({}, state, {
        rows: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.rows), [{
          img: '',
          img_vk: '',
          title: '',
          descr: '',
          url: '',
          button: '',
          button_url: ''
        }])
      });

    case 'deleteCard':
      state.rows.splice(action.payload, 1);
      return _objectSpread({}, state);

    case 'itemLeft':
      state.rows.splice(action.payload - 1, 0, state.rows.splice(action.payload, 1)[0]);
      return _objectSpread({}, state);

    case 'itemRight':
      state.rows.splice(action.payload + 1, 0, state.rows.splice(action.payload, 1)[0]);
      return _objectSpread({}, state);

    case 'sort':
      var _action$payload = action.payload,
          oldIndex = _action$payload.oldIndex,
          newIndex = _action$payload.newIndex;

      var newTiles = Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.rows);

      newTiles.splice(oldIndex, 1);
      newTiles.splice(newIndex, 0, state.rows[oldIndex]);
      state.rows = newTiles;
      return _objectSpread({}, state);

    case 'sub':
      var item_id = action.payload.item_id;
      newRows[item_id].button = action.payload.data.button;
      newRows[item_id].button_url = action.payload.data.button_url;
      return _objectSpread({}, state, {
        rows: newRows
      });

    default:
      throw new Error();
  }
}

