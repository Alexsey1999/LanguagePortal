    getHash: function getHash() {
      return App.state.hash;
    },
    setDefaultSubscription: function setDefaultSubscription(state, callback) {
      App.setState({
        defaultSubscription: state
      }, function () {
        callback && callback();
      });
    },
    getDefaultSubscriptionId: function getDefaultSubscriptionId() {
      return App.state.defaultSubscription;
    },
