function reducer(state, action) {
  switch (action.type) {
    case 'title':
      return _objectSpread({}, state, {
        title: action.payload[0],
        title_url: action.payload[1]
      });

    case 'more':
      return _objectSpread({}, state, {
        more: action.payload[0],
        more_url: action.payload[1]
      });

    case 'add':
      return _objectSpread({}, state, {
        body: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.body), [{
          img: action.payload.img,
          img_vk: action.payload.img_vk,
          title: action.payload.title,
          url: action.payload.url
        }])
      });

    case 'delete':
      state.body.splice(action.payload, 1);
      return _objectSpread({}, state);

    case 'newList':
      return _objectSpread({}, state, {
        body: Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(action.payload)
      });

    default:
      throw new Error();
  }
}

