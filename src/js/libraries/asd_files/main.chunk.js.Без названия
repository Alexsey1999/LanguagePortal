    /**
     * Generate data for the request in FormData object
     * @param data object
     * @returns requestData object
     */
    value: function _createRequestData(data) {
      var requestData = new FormData();

      var _loop = function _loop(key) {
        if (data[key] !== null && typeof data[key] !== 'undefined' && data[key] !== '') {
          if (Array.isArray(data[key])) {
            if (data[key].length === 0) {
              requestData.append("".concat(key, "[]"), '');
            } else {
              data[key].forEach(function (i) {
                requestData.append("".concat(key, "[]"), i);
              });
            }
          } else {
            requestData.append(key, data[key]);
          }
        }
      };

      for (var key in data) {
        _loop(key);
      }

      requestData.append('vk_group_id', this.launchParams.params.vk_group_id.toString());

      try {
        requestData.append('params', JSON.stringify(this.launchParams.params));
      } catch (e) {
        console.log(e);
      }

      return requestData;
    }
