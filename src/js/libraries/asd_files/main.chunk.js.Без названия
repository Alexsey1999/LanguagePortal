var getSubscriptionPrice = function getSubscriptionPrice(subscription) {
  if (!subscription || false === subscription.pay || !subscription.pay_price) {
    return '';
  }

  if (subscription.pay_period !== 0) {
    return "".concat(subscription.pay_price, " \u0440/").concat(subscription.pay_period == 2 ? '' : '');
  } else {
    return "".concat(subscription.pay_price);
  }
};
/**
 *         maxLength
 *   html 
 * @param text 
 */

var cropDescription = function cropDescription(text) {
  var maxLength = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 200;

  if (typeof text !== 'string') {
    return '';
  }

  if (text.length <= maxLength) {
    return text;
  }

  var newText = text;

  while (newText.length > maxLength) {
    if (/<br\s*\/?>$/gm.test(newText)) {
      newText = newText.replace(/<br\s*\/?>$/gm, '');
    } else {
      newText = newText.slice(0, -1);
    }
  }

  return "".concat(newText.trim(), "...");
};
/**
 *     
 * @param subscription 
 */

var getPayPeriod = function getPayPeriod(subscription) {
  if (!subscription.pay_period) {
    return null;
  }

  if (subscription.pay_period === _Constants__WEBPACK_IMPORTED_MODULE_0__["PAY_PERIOD_DAY"] && subscription.pay_period_count > 0) {
    return "/ ".concat(subscription.pay_period_count, " \u0434\u043D");
  }

  if (subscription.pay_period === _Constants__WEBPACK_IMPORTED_MODULE_0__["PAY_PERIOD_MONTH"] && subscription.pay_period_count > 0) {
    return "/  ".concat(subscription.pay_period_count, " \u043C\u0435\u0441");
  }

  return '';
};

