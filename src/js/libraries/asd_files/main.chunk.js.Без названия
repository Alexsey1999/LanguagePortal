function isYouTubeUrl(url) {
  var match = url.match(/^(https?:\/\/)?((www\.)?youtube\.com)\/.+$/gi);
  return match && match.length > 0;
}
function isYouTubeShortUrl(url) {
  var match = url.match(/^(https?:\/\/)?((www\.)?youtu\.?be)\/.+$/gi);
  return match && match.length > 0;
}
function isVimeoUrl(url) {
  var match = url.match(/^https:\/\/(www\.)?vimeo.com\/.+$/gi);
  return match && match.length > 0;
}
function isUrl(url) {
  var match = url.match(/https?:\/\/(www\.)?[-a-zA-Z0-9@:%._+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_+.~#?&/=]*)\/.*/gi);
  return match && match.length > 0;
}
function isValidBannerUrl(url) {
  return isYouTubeUrl(url) || isYouTubeShortUrl(url) || isVimeoUrl(url) || isUrl(url);
}
function isValidYouTubeVideoUrl(url) {
  var match = url.match(/http(?:s?):\/\/(?:www\.)?youtu(?:be\.com\/watch\?v=|\.be\/)([\w\-_]*)(&(amp;)?[\w?=]*)?/gi);

  if (!match) {
    return false;
  }

  return match && match.length > 0;
}
function resolveBannerUrl(url) {
  var vkey = '';
  var vtype = 0;
  var btype = '';
  var burl = '';

  if (isYouTubeUrl(url)) {
    //     
    var match = url.match(/(\Wv=([\w-]+)|\/embed\/([\w-]+))/gi);

    if (match && match.length > 0) {
      var key = match[0].replace('?v=', '').replace('/embed/', '');
      vkey = key;
      vtype = 1;
      btype = 'youtube';
      burl = "https://www.youtube.com/embed/".concat(key, "?autoplay=0&rel=0&controls=0&showinfo=0&fs=0&loop=1&playlist=").concat(key);
    }
  } else if (isYouTubeShortUrl(url)) {
    //    
    var _match = url.match(/\/youtu\.be\/([\w-]+)/gi);

    if (_match && _match.length > 0) {
      var _key = _match[0].replace('/youtu.be/', '');

      vkey = _key;
      vtype = 1;
      btype = 'youtube';
      burl = "https://www.youtube.com/embed/".concat(_key, "?autoplay=0&rel=0&controls=0&showinfo=0&fs=0&loop=1&playlist=").concat(_key);
    }
  } else if (isVimeoUrl(url)) {
    //    Vimeo
    var _match2 = url.match(/(videos|video|channels|\.com)\/([\d]+)/gi);

    if (_match2 && _match2.length > 0) {
      var _key2 = _match2[0].replace('channels/', '').replace('.com/', '').replace('videos/', '').replace('video/', '');

      vkey = _key2;
      vtype = 2;
      btype = 'vimeo';
      burl = "https://player.vimeo.com/video/".concat(_key2, "?autoplay=0&loop=1");
    }
  } else {
    //       
    vkey = url;
    vtype = 0;
    btype = 'img';
    burl = url;
  }

  return {
    video_key: vkey,
    video_type: vtype,
    banner_url: burl,
    banner_type: btype,
    raw_url: url
  };
}

