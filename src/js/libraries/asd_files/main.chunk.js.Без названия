              _this.props.mainPopout.close();

              _this.setState({
                start_subscribe: false
              });

              if (_this.state.opts.pay && (!_this.props.opts.paid || _this.props.opts.pay_period > 0)) _this.prepare_pay();else _this.subscribe();
              break;

            /**
             *        
             */

            case 'VKWebAppAllowMessagesFromGroupFailed':
              _this.props.mainPopout.close();

              _this.setState({
                start_subscribe: false
              });

              break;

            /**
             *     ( ""|"")
             */

            case 'VKWebAppViewHide':
              _this.props.mainPopout.close();

              _this.setState({
                start_subscribe: false
              });

              break;

            /**
             *      
             */

            case 'VKWebAppViewRestore':
              _this.setState({
                start_subscribe: false
              });

              break;

            case 'VKWebAppScrollResult':
              break;

            default:
              // this.logger.error({
              //     message: `Bridge event log: ${JSON.stringify(e)}`,
              //     code: 999
              // }, 'bridge_log', 'Subscription.jsx');
              break;
          }

          if (!e.detail.data) {// this.logger.error({
            //     message: `Bridge event error - undefined data subscribe - ${window.navigator.userAgent} - ${JSON.stringify(e)}`,
            //     code: 997
            // }, 'bridge_error', 'Subscription.jsx');
          }
        }

        if (_this.start_pay) {
          switch (e.detail.type) {
            case 'VKWebAppOpenPayFormResult':
              _this.props.mainPopout.close();

              _this.start_pay = false;
              var extra = JSON.parse(e.detail.data.extra); //this.check_order(extra.group_pay_id);

              setTimeout(function () {
                _this.subscribe(extra.group_pay_id);
              }, 20);
              break;

            case 'VKWebAppOpenPayFormFailed':
              _this.props.mainPopout.close();

              _this.start_pay = false;
              break;

            default:
          }
        }
      }
    });
    return _this;
  }

