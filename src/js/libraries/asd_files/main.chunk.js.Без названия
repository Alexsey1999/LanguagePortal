        return _objectSpread({}, item);
      });
      newTiles[action.payload[0]].button = action.payload[1][0];
      newTiles[action.payload[0]].button_url = action.payload[1][1];
      return _objectSpread({}, state, {
        tiles: newTiles
      });

    case 'img':
