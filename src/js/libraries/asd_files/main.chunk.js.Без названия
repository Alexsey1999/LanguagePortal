var TextToHtml = function TextToHtml(html) {
  if (!html) return html;
  html = html.trim();
  if (!html.length) return html;
  var arr = html.split(/<br\s*\/?>/i);
  var i = 0;
  return arr.reduce(function (el, a) {
    i++;
    return el.concat(a.trim(), react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement("br", {
      key: i,
      __source: {
        fileName: _jsxFileName,
        lineNumber: 14
      },
      __self: this
    }));
  }, []);
};

var lineBreaksToHtmlBreaks = function lineBreaksToHtmlBreaks(text) {
  if (!text) return '';
  return text.replace(_Constants__WEBPACK_IMPORTED_MODULE_1__["TEXT_LINE_BREAKS_REGEXP"], '<br>');
};
var simpleTextToHtml = function simpleTextToHtml(text) {
  return TextToHtml(lineBreaksToHtmlBreaks(text));
};
var simpleTextToHtmlLanding = function simpleTextToHtmlLanding(text) {
  text = text.replace(/\[link\]([^|]+)\|?([^|]+)?\[\/link\]/im, function (match) {
    var link_data = match.replace(/\[link\]/im, '').replace(/\[\/link\]/im, '').split('|');
    return "<a href=\"".concat(link_data[1], "\" target=\"_blank\" rel=\"noopener noreferrer\" title=\"\u041F\u0435\u0440\u0435\u0439\u0442\u0438 \u043F\u043E \u0441\u0441\u044B\u043B\u043A\u0435\">").concat(link_data[0], "</a>");
  });
  return lineBreaksToHtmlBreaks(text);
};
