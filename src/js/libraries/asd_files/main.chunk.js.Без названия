var AnalyticsInstance = {
  goal: function goal(identifier) {
    var type = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'widgets';

    try {
      this.sendYandex({
        target: identifier
      });
      this.sendGoogle({
        action: identifier,
        category: type === 'widgets' ? ANALITICS_WIDGET_CATEGORY : ANALITICS_PAGES_CATEGORY
      });
    } catch (e) {
      console.log(e);
    }
  },
  sendYandex: function sendYandex(goal) {
    if (typeof window.ym !== 'undefined') {
      window.ym(METRIKA_COUNTER, 'reachGoal', goal.target);
    }
  },
  sendGoogle: function sendGoogle(goal) {
    if (typeof window.gtag !== 'undefined') {
      window.gtag('event', goal.action, {
        event_category: goal.category,
        event_action: goal.action
      });
    }
  },
  push: function push(event) {
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push(event);
  }
};
