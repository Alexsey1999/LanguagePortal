function ownKeys(object, enumerableOnly) { var keys = Object.keys(object); if (Object.getOwnPropertySymbols) { var symbols = Object.getOwnPropertySymbols(object); if (enumerableOnly) symbols = symbols.filter(function (sym) { return Object.getOwnPropertyDescriptor(object, sym).enumerable; }); keys.push.apply(keys, symbols); } return keys; }

function _objectSpread(target) { for (var i = 1; i < arguments.length; i++) { var source = arguments[i] != null ? arguments[i] : {}; if (i % 2) { ownKeys(source, true).forEach(function (key) { Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_defineProperty__WEBPACK_IMPORTED_MODULE_1__["default"])(target, key, source[key]); }); } else if (Object.getOwnPropertyDescriptors) { Object.defineProperties(target, Object.getOwnPropertyDescriptors(source)); } else { ownKeys(source).forEach(function (key) { Object.defineProperty(target, key, Object.getOwnPropertyDescriptor(source, key)); }); } } return target; }

function reducer(state, action) {
  var newRows = state.rows.map(function (item) {
    return _objectSpread({}, item);
  });

  switch (action.type) {
    case 'title':
      return _objectSpread({}, state, {
        title: action.payload[0],
        title_url: action.payload[1]
      });

    case 'more':
      return _objectSpread({}, state, {
        more: action.payload[0],
        more_url: action.payload[1]
      });

    case 'img':
      newRows[action.payload.cardID].img = action.payload.img;
      newRows[action.payload.cardID].icon_id = action.payload.img_vk;
      return _objectSpread({}, state, {
        rows: newRows
      });

    case 'button':
      newRows[action.payload[0]].button = action.payload[1][0];
      newRows[action.payload[0]].button_url = action.payload[1][1];
      return _objectSpread({}, state, {
        rows: newRows
      });

    case 'item':
      newRows[action.payload[0]].title = action.payload[1].title;
      newRows[action.payload[0]].url = action.payload[1].url;
      newRows[action.payload[0]].descr = action.payload[1].descr;
      newRows[action.payload[0]].geo = action.payload[1].geo;
      newRows[action.payload[0]].time = action.payload[1].time;
      return _objectSpread({}, state, {
        rows: newRows
      });

    case 'add':
      return _objectSpread({}, state, {
        rows: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(newRows), [{
          img: '',
          icon_id: '',
          title: '',
          url: '',
          descr: '',
          button: '',
          button_url: '',
          geo: '',
          time: ''
        }])
      });

    case 'delete':
      newRows.splice(action.payload, 1);
      return _objectSpread({}, state, {
        rows: newRows
      });

    case 'update':
      return _objectSpread({}, state, {
        rows: action.payload.map(function (item) {
          return _objectSpread({}, item);
        })
      });

    default:
      throw new Error();
  }
}

