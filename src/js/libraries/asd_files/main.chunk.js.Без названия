var initialSettings = function initialSettings(App) {
  return {
    getCurrentSlide: function getCurrentSlide() {
      return App.state.initialSettingsSlide;
    },
    next: function next() {
      var nextSlide = App.state.initialSettingsSlide + 1;
      App.setState({
        initialSettingsSlide: nextSlide
      });
    },
    prev: function prev() {
      var prevSlide = App.state.initialSettingsSlide - 1;

      if (prevSlide < 0) {
        prevSlide = 0;
      }

      App.setState({
        initialSettingsSlide: prevSlide
      });
    }
  };
};
