function ownKeys(object, enumerableOnly) { var keys = Object.keys(object); if (Object.getOwnPropertySymbols) { var symbols = Object.getOwnPropertySymbols(object); if (enumerableOnly) symbols = symbols.filter(function (sym) { return Object.getOwnPropertyDescriptor(object, sym).enumerable; }); keys.push.apply(keys, symbols); } return keys; }

function _objectSpread(target) { for (var i = 1; i < arguments.length; i++) { var source = arguments[i] != null ? arguments[i] : {}; if (i % 2) { ownKeys(source, true).forEach(function (key) { Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_defineProperty__WEBPACK_IMPORTED_MODULE_0__["default"])(target, key, source[key]); }); } else if (Object.getOwnPropertyDescriptors) { Object.defineProperties(target, Object.getOwnPropertyDescriptors(source)); } else { ownKeys(source).forEach(function (key) { Object.defineProperty(target, key, Object.getOwnPropertyDescriptor(source, key)); }); } } return target; }


var createUrlForWidget = function createUrlForWidget(url) {
  if (!url || url.length === 0) {
    return '';
  }

  var u = url;
  if (u.indexOf('https://') >= 0) u = u.slice(8, u.length);
  if (u.indexOf('http://') >= 0) u = u.slice(7, u.length);

  if (u.indexOf('vk.cc/') === 0) {
    var key = u.replace('vk.cc/', '');
    return "https://vk.com/cc?act=go&key=".concat(key);
  } else {
    return url;
  }
};
var widgetHasUnpublishedState = function widgetHasUnpublishedState(widget) {
  var targetWidget = _objectSpread({}, widget);

  if (!targetWidget.last_published_state || typeof targetWidget.last_published_state !== 'string') return false;

  try {
    var lastState = JSON.parse(widget.last_published_state);
    if (!lastState.code) return false;
    var serializedCurrentState = JSON.stringify({
      code: widget.code,
      audience: widget.audience
    });
    return targetWidget.last_published_state !== serializedCurrentState;
  } catch (e) {
    return false;
  }
};
var getWidgetsWithLastPublishedState = function getWidgetsWithLastPublishedState(widgets) {
  return widgets.map(function (item) {
    if (widgetHasUnpublishedState(item)) {
      var itemInPublishedState = _objectSpread({}, item);

      try {
        var lastPublishedState = JSON.parse(item.last_published_state);
        itemInPublishedState.code = lastPublishedState.code;
        itemInPublishedState.audience = lastPublishedState.audience;
        return itemInPublishedState;
      } catch (e) {
        console.log(e);
        return item;
      }
    } else {
      return item;
    }
  });
};
var widgetHasAudience = function widgetHasAudience(widget) {
  var audience = _objectSpread({}, widget.audience);

  if (Object.keys(audience).length === 0 || audience.length === 0) return false; // 

  if (audience.sex && audience.sex !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.ageFrom && audience.ageFrom !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.ageTo && audience.ageTo !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.names && audience.names !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.platform && audience.platform.length > 0 || audience.bDay && audience.bDay !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.surnames && audience.surnames !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.userCoupleInterests && audience.userCoupleInterests !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.userInterests && audience.userInterests !== _Constants__WEBPACK_IMPORTED_MODULE_1__["AUDIENCE_ANY"] || audience.communityIn && audience.communityIn.length > 0 || audience.communityOut && audience.communityOut.length > 0 || audience.users && audience.users.length > 0 || audience.currentCity && audience.currentCity.length > 0 || audience.currentCountry && audience.currentCountry.length > 0 || audience.relation && audience.relation.length > 0) {
    return true;
  } else {
    return false;
  }
};

var isEmoji = function isEmoji(char) {
  return char.match(_Constants__WEBPACK_IMPORTED_MODULE_1__["EMOJI_REGEX"]);
};

var isZWJ = function isZWJ(char) {
  return char.codePointAt(0).toString(16) === _Constants__WEBPACK_IMPORTED_MODULE_1__["ZERO_WIDTH_JOINER"];
};

var isSpecialChar = function isSpecialChar(char) {
  return char.match(_Constants__WEBPACK_IMPORTED_MODULE_1__["SPECIAL_CHARS_REGEX"]);
};
/**
 *          
 *    ZWJ      Emoji
 * @param string 
 */


var trimUnnecessaryCharacters = function trimUnnecessaryCharacters(string) {
  var hasEmojis = isEmoji(string);
  string = string.replace(/[\u200c]/gi, ''); //   zero width non joiner

  string = string.replace(_Constants__WEBPACK_IMPORTED_MODULE_1__["SPACES_ENTITIES"], ' '); //     

  if (!hasEmojis) {
    //     emoji
    string = string.replace(_Constants__WEBPACK_IMPORTED_MODULE_1__["JOINERS_ENTITIES"], ''); //   zwj
  }

  return string.trim();
};
/**
 *    ,    VK   
 * @param string 
 */

var countStringLength = function countStringLength(string) {
  if (typeof string !== 'string') {
    console.warn("WARN: countStringLength - inappropriate data type ".concat(typeof string));
    return 0;
  }

  ;
  string = string.replace(/(\[gender\]|\[\/gender\])/gi, ''); //     

  var chars = Array.from(string);
  var decodeStringLength = 0;
  chars.forEach(function (char) {
    if (isZWJ(char)) {
      decodeStringLength += 5;
    } else if (char.match(/[\u200c]/gi)) {
      decodeStringLength += 6;
    } else if (char.match(/[\u2003]/gi)) {
      decodeStringLength += 6;
    } else if (char.match(/[\u2009]/gi)) {
      decodeStringLength += 8;
    } else if (isSpecialChar(char)) {
      decodeStringLength += _Constants__WEBPACK_IMPORTED_MODULE_1__["SPECIAL_CHAR_COUNT"];
    } else if (isEmoji(char)) {
      decodeStringLength += _Constants__WEBPACK_IMPORTED_MODULE_1__["EMOJI_COUNT"];
    } else {
      decodeStringLength += char.length;
    }
  });
  return decodeStringLength;
};
/**
 *    VK    , 
 *      
 * @param string 
 */

var cropStringForVk = function cropStringForVk(string) {
  var maxLength = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : _Constants__WEBPACK_IMPORTED_MODULE_1__["MAX_STRING_LENGTH"];
  string = trimUnnecessaryCharacters(string);
  var decodedStringLength = countStringLength(string);

  if (decodedStringLength <= maxLength) {
    return string;
  }

  var newString = string;

  while (countStringLength(newString) > maxLength - 4 || isEmoji(newString[newString.length - 1]) || isZWJ(newString[newString.length - 1])) {
    newString = newString.slice(0, -1);
  }

  return "".concat(newString.trim(), "...");
};
var scopeReplace = function scopeReplace(scope) {
  var newScope = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : "";
  var arr = decodeURIComponent(scope).split(',');
  if (!!newScope) arr.push(newScope);
  if (arr.indexOf("notify") >= 0) arr.splice(arr.indexOf("notify"), 1);
  return arr.join(',');
};
/**
 *   
 * @param obj 
 */

var deepFreeze = function deepFreeze(obj) {
  //      obj
  var propNames = Object.getOwnPropertyNames(obj); //      

  propNames.forEach(function (name) {
    var prop = obj[name]; //   prop,   

    if (typeof prop == 'object' && prop !== null) deepFreeze(prop);
  }); //    obj (  ,    )

  return Object.freeze(obj);
};
var setHash = function setHash(data) {
  var hash = '';

  if (data.list) {
    hash += "".concat(data.list, "|");
  }

  if (data.view) {
    hash += "".concat(data.view, "|");
  }

  if (data.panel) {
    hash += "".concat(data.panel, "|");
  }

  if (data.widget_id) {
    hash += "".concat(data.widget_id, "|");
  }

  if (data.page_id) {
    hash += "".concat(data.page_id, "|");
  }

  if (data.type && data.type_api) {
    hash += "".concat(data.type, "-").concat(data.type_api);
  }

  if (data.tab) {
    hash += "".concat(data.tab, "|");
  }

  if (data.s_id) {
    hash += "".concat(data.s_id, "|");
  }

  if (data.block_type) {
    hash += "".concat(data.block_type, "|");
  }

  if (data.block_id) {
    hash += "".concat(data.block_id, "|");
  }

  window.location.hash = hash;
  return hash;
};
/**
 * 
 * @param {string} hash 
 */

var parseHash = function parseHash(hash) {
  var res = {
    list: 'subscriptions',
    view: 'main',
    panel: '',
    widget_id: ''
  };
  if (!hash) return res;
  var clearValue = hash.substring(1);
  var parts = clearValue.split('|');

  if (parts[0]) {
    if (parts[0].indexOf('s=') >= 0) {
      res.s_id = parts[0].replace('s=', '');
      return res;
    }

    if (parts[0].indexOf('d=') >= 0 || parts[0] === 'unsubscribe') {
      res.list = 'mysubscriptions';
      return res;
    }

    if (['subscriptions', 'mysubscriptions', 'widgets', 'app-settings', 'pages'].indexOf(parts[0]) >= 0) {
      res.list = parts[0];
    } else {
      res.list = 'subscriptions';
    }
  }

  if (parts[1]) {
    res.view = parts[1];

    if (parts[1] === 'initial-settings') {
      res.view = 'initial-settings';
      return res;
    }
  }

  if (parts[2]) {
    res.panel = parts[2];
  }

  if (parts[3]) {
    res.widget_id = parts[3];
  }

  if (parts[1] === 'widgets' && parts[2] === 'name') {
    delete res.widget_id;
    res.type = parts[3].split('-')[0];
    res.type_api = parts[3].split('-')[1];
  }

  if ((parts[0] === 'subscriptions' || parts[0] === 'mysubscriptions') && parts[1]) {
    delete res.view;
    res.s_id = parts[1];
  }

  if (parts[4] && parts[2] !== 'blocks-edit') {
    res.tab = parts[4];
  }

  if (parts[0] === 'pages' && parts[3]) {
    delete res.widget_id;
    res.page_id = parts[3];
  }

  if (parts[2] === 'blocks-edit' && parts[4]) {
    res.block_id = parts[4];
  }

  if (parts[2] === 'blocks-items' && parts[3]) {
    delete res.page_id;
    delete res.widget_id;
    res.block_type = parts[3];
  }

  return res;
};
/**
 *  -    string   screen_name
 * @param string
 * @TODO -     - https://vk.com/faq18038 
 */

var isScreenName = function isScreenName(string) {
  // Allow only latin characters with lodash
  return /^[a-z]+[a-z_]+/gmi.test(string.toLowerCase()) && string.length >= 4;
};
/**
 *  -       VK
 * @param string 
 */

var isValidUrl = function isValidUrl(string) {
  return /^(?:https?:\/\/)?(?:m.)?vk.com\/([^\s]+)/gmi.test(string.toLowerCase());
};
/**
 *  -     
 * @param value 
 */

var isValidLink = function isValidLink(value) {
  //       http\https
  return /^((?:(?:(?:https?|ftp):)?\/\/))?(?:\S+(?::\S*)?@)?(?:(?!(?:10|127)(?:\.\d{1,3}){3})(?!(?:169\.254|192\.168)(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})))(?::\d{2,5})?(?:[/?#]\S*)?$/i.test(value);
};
/**
 *      ,   . , apiclub, andrew  rules_of_war.
 * @param url 
 * @returns {string} -  screen_name,   .  ,    
 */

var getScreenNameFromUrl = function getScreenNameFromUrl(url) {
  if (!isValidUrl(url)) {
    console.warn("GetScreenNameFromUrl: URL - ".concat(url, " - is not valid vk url"));
    return '';
  }

  var screen_name = url.replace(/^(?:https?:\/\/)?(?:m.)?vk.com\//gmi, '');

  if (!isScreenName(screen_name)) {
    console.warn("GetScreenNameFromUrl: parsed screen name - ".concat(screen_name, " - is not valid vk screen name"));
    return '';
  }

  return screen_name;
};
/**
 *  id    
 * - 
 * - 
 * - 
 * - 
 * @param url string
 */

var getVkIdFromUrl = function getVkIdFromUrl(url) {
  var result = {
    type: '',
    value: 0
  };
  var matches = /^(?:https?:\/\/)?(?:m.)?vk.com\/(id|public|club|event)(\d{1,20})/gi.exec(url);

  if (matches) {
    result.type = matches[1];
    result.value = parseInt(matches[2]);
  }

  return result;
};
var dateToISOFormat = function dateToISOFormat(dateTime) {
  try {
    var parts = dateTime.split(' ');
    var date = parts[0].split('.').reverse().join('-');
    var time = parts[1]; //          

    if (!time || date.split('-').length < 3) {
      return '';
    }

    return "".concat(date, "T").concat(time, ":00");
  } catch (e) {
    console.warn("DateToIsoFormat: Wrong dateTime format - ".concat(typeof dateTime));
    return '';
  }
};
var daysBetween = function daysBetween(startDate, endDate) {
  if (!(startDate instanceof Date) || !(endDate instanceof Date)) {
    console.warn("DaysBetween: date wrong format");
    return 0;
  } // The number of milliseconds in all UTC days (no DST)


  var oneDay = 1000 * 60 * 60 * 24; // A day in UTC always lasts 24 hours (unlike in other time formats)

  var start = Date.UTC(endDate.getFullYear(), endDate.getMonth(), endDate.getDate());
  var end = Date.UTC(startDate.getFullYear(), startDate.getMonth(), startDate.getDate()); // so it's safe to divide by 24 hours

  return (start - end) / oneDay;
};
/**
 *    ( )    "1 ", "38 "
 * @param days number
 */

var getDaysCount = function getDaysCount(days) {
  if (typeof days !== 'number') {
    console.warn("GetDaysCount helper: wrong days format - ".concat(typeof days));
    return '';
  } // ,    


  if (days < 0) return '';

  if (days === 1 || days % 10 === 1 && days !== 11) {
    return "".concat(days, " \u0434\u0435\u043D\u044C");
  } else if (days >= 12 && days <= 14) {
    return "".concat(days, " \u0434\u043D\u0435\u0439");
  } else if (days >= 2 && days <= 4 || days % 10 >= 2 && days % 10 <= 4) {
    return "".concat(days, " \u0434\u043D\u044F");
  } else {
    return "".concat(days, " \u0434\u043D\u0435\u0439");
  }
};
var isIOS = function isIOS() {
  if (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream) {
    if (!!window.indexedDB) {
      return true;
    }

    if (!!window.SpeechSynthesisUtterance) {
      return true;
    }

    if (!!window.matchMedia) {
      return true;
    }

    if (!!window.history && 'pushState' in window.history) {
      return true;
    }

    return true;
  }

  return false;
};
var isVkSubscriptionUrl = function isVkSubscriptionUrl(url) {
  if (!isValidUrl(url)) {
    return false;
  }

  return /^(?:https?:\/\/)?(?:m.)?vk.com\/([^\s]+)#s=\d{1,}/gm.test(url);
};
var parseSubscriptionIdFromUrl = function parseSubscriptionIdFromUrl(url) {
  var matches = /^(?:https?:\/\/)?(?:m.)?vk.com\/([^\s]+)#s=(\d{1,})/gm.exec(url);

  if (!matches || !matches[2]) {
    return false;
  }

  return parseInt(matches[2], 10);
};
var widgetsImagesSetByIds = function widgetsImagesSetByIds(tiles) {
  if (!tiles) return false;
  var res = tiles.filter(function (t) {
    if (!t.icon_id) return false;
    return /^id(-)?\d{1,}$/gm.test(t.icon_id);
  });
  return res.length > 0;
};
var copyToClipboard = function copyToClipboard(value, callback) {
  var textarea;
  var result;

  try {
    textarea = document.createElement('textarea');
    textarea.setAttribute('readonly', true);
    textarea.setAttribute('contenteditable', true);
    textarea.style.position = 'fixed'; // prevent scroll from jumping to the bottom when focus is set.

    textarea.value = value;
    document.body.appendChild(textarea);
    textarea.focus();
    textarea.select();

    var _range = document.createRange();

    _range.selectNodeContents(textarea);

    var sel = window.getSelection();
    sel.removeAllRanges();
    sel.addRange(_range);
    textarea.setSelectionRange(0, textarea.value.length);
    result = document.execCommand('copy');
  } catch (err) {
    console.error(err);
    result = null;
  } finally {
    document.body.removeChild(textarea);
  } // manual copy fallback using prompt


  if (!result) {
    var isMac = navigator.platform.toUpperCase().indexOf('MAC') >= 0;
    var copyHotkey = isMac ? 'C' : 'CTRL+C';
    result = prompt("\u041D\u0430\u0436\u043C\u0438\u0442\u0435 ".concat(copyHotkey), value); // eslint-disable-line no-alert

    if (!result) {
      return false;
    }
  }

  if (callback && typeof callback === 'function') {
    callback();
  }

  return true;
};
