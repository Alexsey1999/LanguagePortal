    key: "createAudience",
    value: function createAudience(widget) {
      var pairs = [];

      var audience = _objectSpread({}, widget.audience);

      for (var criteria in audience) {
        var variants = audience[criteria];

        if (variants === _Constants__WEBPACK_IMPORTED_MODULE_3__["AUDIENCE_ANY"]) {
          continue;
        }

        switch (criteria) {
          case 'sex':
          case 'ageFrom':
          case 'ageTo':
            if (variants === _Constants__WEBPACK_IMPORTED_MODULE_3__["AUDIENCE_ANY"]) continue;else pairs.push("".concat(criteria, ":").concat(variants));
            break;

          case 'platform':
          case 'relation':
            if (variants.length === 0 || variants === _Constants__WEBPACK_IMPORTED_MODULE_3__["AUDIENCE_ANY"]) continue;else pairs.push("".concat(criteria, ":[").concat(variants.join(','), "]"));
            break;

          case 'users':
          case 'communityIn':
          case 'communityOut':
          case 'currentCity':
          case 'currentCountry':
            if (!variants.length || variants.length === 0) continue;else pairs.push("".concat(criteria, ":[").concat(variants.map(function (i) {
              return i.id;
            }).join(','), "]"));
