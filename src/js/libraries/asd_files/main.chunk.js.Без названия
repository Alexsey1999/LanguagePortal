          from = _action$payload.from,
          to = _action$payload.to;

      var newItems = Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(newState.items);

      newItems.splice(from, 1);
      newItems.splice(to, 0, state.items[from]);
      newState.items = newItems;
      return newState;

    case 'remove-item':
