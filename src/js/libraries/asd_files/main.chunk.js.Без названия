        tiles: [].concat(Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_0__["default"])(state.tiles), [{
          img: '',
          icon_id: '',
          title: '',
          descr: '',
          url: '',
          button: '',
          button_url: ''
        }])
      });

    case 'deleteCard':
      newTiles = state.tiles.map(function (item) {
        return _objectSpread({}, item);
      });
      newTiles.splice(action.payload, 1);
      return _objectSpread({}, state, {
        tiles: newTiles
      });

    case 'itemLeft':
