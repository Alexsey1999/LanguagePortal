      key: "getStateManagement",
      value: function getStateManagement() {
        return App.state.pages.stateManage;
      }
    }, {
      key: "undo",
      value: function undo(page_id) {
        var state = _objectSpread({}, App.state.pages);

        var active = state.stateManage.active + 1;

        if (active > state.stateManage.available.length - 1) {
          return false;
        }

        var blocks_edit = state.stateManage.available[active].blocks_edit;
        var page = null;
        var pageIndex = 0;
        state.items.forEach(function (p, i) {
          if (p.id === page_id) {
            page = p;
            pageIndex = i;
          }
        });

        if (!page) {
          return false;
        }

        state.stateManage.active = active;
        page.blocks_edit = blocks_edit;
        state.items[pageIndex] = page;
        App.setState({
          pages: state
        });
      }
    }, {
      key: "redo",
      value: function redo(page_id) {
        var state = _objectSpread({}, App.state.pages);

        var active = state.stateManage.active - 1;

        if (active < 0) {
          return false;
        }

        var blocks_edit = state.stateManage.available[active].blocks_edit;
        var page = null;
        var pageIndex = 0;
        state.items.forEach(function (p, i) {
          if (p.id === page_id) {
            page = p;
            pageIndex = i;
          }
        });

        if (!page) {
          return false;
        }

        state.stateManage.active = active;
        page.blocks_edit = blocks_edit;
        state.items[pageIndex] = page;
        App.setState({
          pages: state
        });
      }
    }, {
      key: "setRecentBlocks",
      value: function setRecentBlocks(items) {
        var state = _objectSpread({}, App.state.pages);

        state.recentBlocks = items;
        App.setState({
          pages: state
        });
      }
    }, {
      key: "getRecentBlocks",
      value: function getRecentBlocks() {
        return Object(C_Users_Alexsey_Desktop_vkapp_node_modules_babel_preset_react_app_node_modules_babel_runtime_helpers_esm_toConsumableArray__WEBPACK_IMPORTED_MODULE_5__["default"])(App.state.pages.recentBlocks);
      }
    }, {
      key: "setHitData",
      value: function setHitData(hitData) {
        App.setState({
          page_hit_data: hitData
        });
      }
    }, {
      key: "getHitId",
      value: function getHitId() {
        return App.state.page_hit_data.hit_id;
      }
    }, {
      key: "getMaxItemsCounters",
      value: function getMaxItemsCounters() {
        return _objectSpread({}, App.state.pagesMaxItemsCounters);
      }
    }, {
      key: "setMaxItemsCounters",
      value: function setMaxItemsCounters(counters) {
        App.setState({
          pagesMaxItemsCounters: counters
        });
      }
    }, {
      key: "setAddBlockMode",
      value: function setAddBlockMode(value) {
        var callback = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : function () {};
        App.setState({
          addBlockMode: value
        }, function () {
          callback && callback();
        });
      }
    }, {
      key: "getAddBlockMode",
      value: function getAddBlockMode() {
        return App.state.addBlockMode;
      }
    }, {
